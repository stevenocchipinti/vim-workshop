{
  "name": "react-vim-wasm",
  "version": "0.1.4",
  "description": "React component for vim.wasm",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "npm-run-all build:tsc build:parcel",
    "build:parcel": "parcel build -d src -o bundle.js ./src/index.js",
    "build:tsc": "tsc -p .",
    "clean": "git clean -fX",
    "lint": "eslint '*.tsx' 'src/*.tsx'",
    "postinstall": "patch-package",
    "start": "npm-run-all build watch",
    "watch": "npm-run-all -p watch:tsc watch:parcel",
    "watch:parcel": "parcel serve -p 1234 -d src -o bundle.js ./src/index.js",
    "watch:tsc": "tsc --watch -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/react-vim-wasm.git"
  },
  "keywords": [
    "react",
    "vim",
    "wasm",
    "WebAssembly",
    "editor"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp> (https://rhysd.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/react-vim-wasm/issues"
  },
  "homepage": "https://github.com/rhysd/react-vim-wasm#readme",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-security": "^1.4.0",
    "file-saver": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "vim-wasm": "~0.0.13"
  }
}
